<!-- hotkey command for rails equal sign and tab -->

<h1>Our Passengers Are Popstars!</h1>

<ul class="name-columns">
  <% @passengers.each do |passenger| %>
  <li>
    <%= link_to(passenger.name, passenger_path(passenger.id), alt:'Link to Show Passenger view')%>
  </li>
  <% end %>
</ul>
