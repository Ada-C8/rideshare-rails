

<div class="wrapper">
  <section class="index">
    <h2> All Passengers</h2>
    <div class="list-passengers">
    <ul>
      <% @passengers.each do |passenger| %>
      <li>
        <%= link_to passenger.name, passenger_path(passenger) %>
      </li>
      <% end %>
    </ul>
  </div>
  </section>
</div>
