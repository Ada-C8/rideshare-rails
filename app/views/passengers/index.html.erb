<h1>Passengers#index</h1>
<% @passengers.each do |passenger| %>
Passenger ID: <%= passenger.id %>
<ul>
  <li>Name:
    <strong><%= link_to(passenger.name, passenger_path(passenger.id)) %></strong>
  </li>
  <li>
    <strong><%= link_to "edit", edit_passenger_path(passenger.id) %></strong>
    <strong><%=link_to "delete", passenger_path(passenger.id), method: :delete, data: { confirm: "ru sure" } %></strong>
  </li>
</ul>
<% end %>
<p>Find me in app/views/passengers/index.html.erb</p>
